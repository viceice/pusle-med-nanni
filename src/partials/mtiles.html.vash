@if (model.title){
<h4>
    @model.title
</h4>
}

<div class="tile-container">
    @model.getTiles(model.path).forEach(function(img) {
        @model.partial('tile', false, { getTile: getTile, src: img.url, label: img.basename.replace(/^\d+_/, ""), size: model.size || 'tile' })
    })
</div>
